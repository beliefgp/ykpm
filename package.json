{
  "name": "ykpm",
  "version": "2.1.5",
  "description": "基于webpak封装的打包工具，集成打包、调试、代理、ajax劫持模拟数据",
  "keywords": [
    "打包",
    "调试",
    "代理"
  ],
  "main": "index.js",
  "bin": {
    "ykpm": "./ykpm"
  },
  "scripts": {
    "test": ""
  },
  "author": "beliefgp",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/beliefgp/ykpm/issues"
  },
  "homepage": "https://github.com/beliefgp/ykpm#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beliefgp/ykpm.git"
  },
  "dependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "colors": "^1.1.2",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "fs-extra": "^0.30.0",
    "glob": "^7.0.3",
    "iconv-lite": "^0.4.13",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mockjs": "^1.0.1-beta2",
    "optimist": "^0.6.1",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "wildcard-named": "0.0.1"
  },
  "devDependencies": {},
  "ykpm": {
    "build": {
      "buildpath": "./build",
      "filespath": "./src/",
      "option": {
        "cssExtract": true,
        "cssAutoprefixer": true,
        "fileLimit": false
      },
      "external": {},
      "alias": {},
      "global": {
        "$": "jquery"
      },
      "lib": {
        "filename": "js/lib.js",
        "jquery": "lib/jquery.min.js"
      },
      "files": [
        "js/live.js"
      ]
    },
    "debug": {
      "contentBase": "./static/",
      "hostname": "local.youku.com",
      "port": 3333,
      "hot": true,
      "proxyFilter": {
        "jscss": "(js|css)"
      },
      "proxy": {
        "http://css.tudou.com/v3/dist/src/": [
          {
            "path": "[all:path]/[alnum:file]_[digit:version].[jscss:extname]",
            "target": "[path]/[file].[extname]"
          },
          {
            "action": "/getJsVersion.action",
            "data": {
              "data|1-10": [
                {
                  "id|+1": 1
                }
              ]
            }
          }
        ],
        "http://portal.zb.youku.com/liveportal/getTest4.action": {
          "data": {
            "data|1-10": [
              {
                "id|+2": 2
              }
            ]
          }
        },
        "http://portal.zb.youku.com/liveportal": {
          "action": "getTest1.action",
          "data": {
            "data|1-10": [
              {
                "id|+2": 2
              }
            ]
          }
        }
      }
    }
  }
}
